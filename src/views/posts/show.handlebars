<div class="container p-5">
    <h1>{{post.title}}</h1>

    <h6>{{post.description}}</h6>

    <p>{{post.content}}</p>

    <div>
        {{#each post.images}}
        <img src="{{this.url}}" alt="post image" width="200px">

        {{/each}}

    </div>

    <span>Atualizado em: {{post.updatedAt}}</span>

</div>